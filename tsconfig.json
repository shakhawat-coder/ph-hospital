{
  "compilerOptions": {
    "module": "esnext", 
    "moduleResolution": "bundler",     
     "target": "ES2023",                          
    "rootDir": "./src",
    "outDir": "./dist",  
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "6.0"
  },
  "include": [
    "./src"
  ],
  "exclude": ["node module","dist"]
  
}


